@prefix jga: <http://ddbj.nig.ac.jp/ontologies/jga/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://ddbj.nig.ac.jp/ontologies/jga/>
    a owl:Ontology ;
    rdfs:label "DDBJ JGA Ontology" ;
    rdfs:comment "Vocabulary for DDBJ Japanese Genotype-phenotype Archive metadata" .

jga:status
    a owl:DatatypeProperty ;
    rdfs:label "status" ;
    rdfs:comment "INSDC record status: always live for JGA" .

jga:properties
    a owl:ObjectProperty ;
    rdfs:label "properties" ;
    rdfs:comment "Original XML structure preserved as a JSON object" .

jga:organism
    a owl:ObjectProperty ;
    rdfs:label "organism" ;
    rdfs:comment "Target organism with taxonomy identifier and scientific name" .

jga:dbXrefs
    a owl:ObjectProperty ;
    rdfs:label "dbXrefs" ;
    rdfs:comment "Cross-references to related entries in other INSDC databases" .
